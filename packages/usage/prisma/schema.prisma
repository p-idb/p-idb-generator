generator custom_generator {
  provider = "prisma-idb-generator"
  output   = "../types"
}

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

model User {
  id    Int      @id @default(autoincrement())
  email String   @unique
  name  String?
  type  UserType
}

model Server {
  id     Int    @id
  status Status
}

enum UserType {
  ADMIN
  USER
}

enum Status {
  ONLINE
  OFFLINE
}
