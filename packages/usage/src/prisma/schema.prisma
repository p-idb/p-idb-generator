datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

generator client {
  provider = "prisma-client-js"
}

generator prismaIDB {
  provider = "prisma-idb-generator"
  output   = "../lib/prisma-idb"
}

enum Status {
  Pending
  Completed
}

model Todo {
  id     Int    @id
  task   String
  status Status
}
