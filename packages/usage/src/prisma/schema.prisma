datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

generator client {
  provider = "prisma-client-js"
}

generator prismaIDB {
  provider = "prisma-idb-generator"
  output   = "../lib/prisma-idb"
}

model Todo {
  id          String  @id @default(uuid())
  task        String
  isCompleted Boolean
  timeToComplete Int
}
